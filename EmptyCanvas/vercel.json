{
  "rewrites": [
    {
      "source": "/orders/new",
      "destination": "/create-order-type.html"
    },
    {
      "source": "/orders/new/details",
      "destination": "/create-order-details.html"
    },
    {
      "source": "/orders/new/products",
      "destination": "/create-order-products.html"
    },
    {
      "source": "/orders/new/review",
      "destination": "/create-order-review.html"
    },
    {
      "source": "/(.*)",
      "destination": "/api/index"
    }
  ],
  "crons": [
    {
      "path": "/api/cron/notifications",
      "schedule": "*/5 * * * *"
    }
  ]
}
